@using AnsibeProject.Models;
@model List<Professor>;
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers


<div class="box" style="border: dashed">
    <input type="button" asp-action="Professor" asp-action="add" name="button0" value="+" />
</div>
@if (Model != null && Model.Count() > 0)
{
   @foreach (var professor in Model)
   {
        <div class="box">
            <div class="tutor">
                <img src="images/pic-2.jpg" alt="">
                <div>
                    <h3>@professor.FullNameInArabic</h3>
                    <span>@professor.Speciality</span>
                </div>
            </div>
            <p>Date of Birth : <span>@professor.DateOfBirth</span></p>
            <p>Email : <span>@professor.Email</span></p>
            <p>Tel Number : <span>@professor.PhoneNumber</span></p>
            <p>Contract : <span>@professor.ContractType</span></p>
            <a href="teacher_profile.html" class="inline-btn"><input type="button" name="button1" value="edit" asp-controller="teachers" asp-action="edit" asp-route-id="@professor.FileNumber" /></a>
        </div>
        
    }
}
